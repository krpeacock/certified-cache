{
  "canisters": {
    "cache_example_backend": {
      "main": "src/cache_example_backend/main.mo",
      "type": "motoko"
    },
    "cache_example_frontend": {
      "dependencies": [
        "cache_example_backend"
      ],
      "frontend": {
